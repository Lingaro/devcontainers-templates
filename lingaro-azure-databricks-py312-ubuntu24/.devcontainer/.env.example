# Azure Service Principal Configuration
# Copy this file to .env and fill in your actual values

# Azure Service Principal Credentials
AZURE_TENANT_ID=your-tenant-id-here
AZURE_CLIENT_ID=your-client-id-here
AZURE_CLIENT_SECRET=your-azure-secret-here

# Databricks Configuration (optional - configure as needed)
DATABRICKS_HOST=https://your-databricks-workspace.azuredatabricks.net
DATABRICKS_TOKEN=your-databricks-token-here

# GitHub Container Registry
IMAGE=ghcr.io/lingaro/lingaro-azure-py312-ubuntu24:latest

# Common API Key for all services (replace with your actual key)
API_KEY=

# Anthropic API Key (see https://www.anthropic.com/index/claude-api)
ANTHROPIC_AUTH_TOKEN=${API_KEY}
# ANTHROPIC_API_KEY=${API_KEY} # Legacy, use ANTHROPIC_AUTH_TOKEN instead
ANTHROPIC_BASE_URL=https://llm.lingarogroup.com
ANTHROPIC_MODEL=vertex_ai/claude-sonnet-4

# Qwen3 Coder API Key (see https://qwen3.com/docs/api/)
OPENAI_API_KEY=${API_KEY}
OPENAI_BASE_URL=https://llm.lingarogroup.com
OPENAI_MODEL=qwen3-coder
