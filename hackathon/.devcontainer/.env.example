# Copy this file to .env and fill in your actual values

# Azure Service Principal Credentials
AZURE_TENANT_ID=your-tenant-id-here
AZURE_CLIENT_ID=your-client-id-here
AZURE_CLIENT_SECRET=your-azure-secret-here

# GitHub Container Registry
IMAGE=ghcr.io/lingaro/hackathon:latest
 
# Anthropic API Key (see https://www.anthropic.com/index/claude-api)
ANTHROPIC_API_KEY=
ANTHROPIC_BASE_URL=https://llm.lingarogroup.com
ANTHROPIC_AUTH_TOKEN=
ANTHROPIC_MODEL=vertex_ai/claude-sonnet-4
 
# Qwen3 Coder API Key (see https://qwen3.com/docs/api/)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://llm.lingarogroup.com
OPENAI_MODEL=qwen3-coder

# Additional environment variables
ENV_VAR_EXAMPLE=your-value-here
